#include <stdio.h>
int main() {
    int arr[] = {2, 4, 6, 8, 10};
    int search_element = 6;
    int n = sizeof(arr) / sizeof(arr[0]);
    int i, found = 0;
    for (i = 0; i < n; i++) {
        if (arr[i] == search_element) {
            printf("Element found at index: %d\n", i);
            found = 1;
            break;
        }
    }
    if (!found) {
        printf("Element not found\n");
    }
    return 0;
}
