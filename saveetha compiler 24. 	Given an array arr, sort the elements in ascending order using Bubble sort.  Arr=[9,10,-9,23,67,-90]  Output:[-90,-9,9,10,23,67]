#include<stdio.h>
void bubblesort(int arr[],int n){
  for(int i=0;i<n-1;i++){
    for(int j=0;j<n-1;j++){
      if(arr[j]>arr[j+1])
      {
        int temp=arr[j];
        arr[j]=arr[j+1];
        arr[j+1]=temp;
      }}
    }
  }
int main(){
  int arr[]={9,10,-9,23,67,-90};
  int n=sizeof(arr)/sizeof(arr[0]);
 bubblesort(arr,n);
  printf("sorted array : ");
  for(int i=0;i<n;i++)
  {
    printf("%d ",arr[i]);
  }
  return 0;
}
